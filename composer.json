{
    "name": "pxgamer/diffchecker",
    "type": "library",
    "description": "An command-line tool for the DiffChecker API.",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "symfony/console": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "pxgamer\\DiffChecker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pxgamer\\DiffChecker\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "bin": [
        "bin/diffchecker"
    ]
}
